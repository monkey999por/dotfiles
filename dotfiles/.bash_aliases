#! /bin/bash

# ~/.bash_aliases

# ll
alias l='ls -laF --time-style="+%Y-%m-%d %H:%M:%S"'

# cd
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# windows command prompt like
alias cls='clear'

#windows extentions
alias sakura="sakura.exe"

# man page as japanese
alias mj="man --locale=ja_JP.UTF-8"
