#! /bin/bash

# ~/.bash_aliases

# ll
alias l='ls -alF'

# cd
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# windows command prompt like
alias cls='clear'

#windows extentions
alias chrome="chrome.exe"
alias sakura="sakura.exe"

alias s='sakura.exe'

# man page as japanese
alias manj="man --locale=ja_JP.UTF-8"
