[alias]
        a = add
        b = branch
        c = checkout
        ch = cherry-pick
        d = diff
        l = log --graph --all --format='%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset%s'
        rmh = reset --mixed HEAD
        rhh = reset --hard HEAD
        s = status
[core]
        autocrlf = false
[init]
	defaultBranch = main
